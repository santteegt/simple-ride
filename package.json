{
    "name": "simple-ride",
    "version": "0.0.1",
    "author": "Santiago Gonzalez",
    "homepage": "https://santteegt.github.io/",
    "private": true,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "lint": "ionic-app-scripts lint",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "meteor-client:bundle": "meteor-client bundle -s api/ -c meteor-client.config.json",
        "api": "cd api && export ROOT_URL=http://simpleride-ec.com && meteor run --settings private/settings.json",
        "api:reset": "cd api && export ROOT_URL=http://simpleride-ec.com && meteor reset"
    },
    "dependencies": {
        "@angular/common": "4.1.2",
        "@angular/compiler": "4.1.2",
        "@angular/compiler-cli": "4.1.2",
        "@angular/core": "4.1.2",
        "@angular/forms": "4.1.2",
        "@angular/http": "4.1.2",
        "@angular/platform-browser": "4.1.2",
        "@angular/platform-browser-dynamic": "4.1.2",
        "@ionic-native/app-version": "^4.1.0",
        "@ionic-native/background-geolocation": "^3.14.0",
        "@ionic-native/camera": "^3.14.0",
        "@ionic-native/core": "3.10.2",
        "@ionic-native/geolocation": "^3.12.1",
        "@ionic-native/keyboard": "^3.12.1",
        "@ionic-native/push": "^4.0.1",
        "@ionic-native/social-sharing": "^4.1.0",
        "@ionic-native/splash-screen": "3.10.2",
        "@ionic-native/status-bar": "3.10.2",
        "@ionic/storage": "2.0.1",
        "@ionic2-extra/calendar": "^0.1.1",
        "angular2-moment": "^1.3.3",
        "babel-runtime": "^6.23.0",
        "cordova-android": "^6.2.3",
        "cordova-ios": "^4.4.0",
        "cordova-plugin-add-swift-support": "^1.6.0",
        "cordova-plugin-app-version": "^0.1.9",
        "cordova-plugin-camera": "^2.4.1",
        "cordova-plugin-compat": "^1.1.0",
        "cordova-plugin-console": "^1.0.7",
        "cordova-plugin-crosswalk-webview": "^2.3.0",
        "cordova-plugin-device": "^1.1.6",
        "cordova-plugin-facebook4": "git+https://github.com/darkbasic/cordova-plugin-facebook4.git",
        "cordova-plugin-file": "^4.3.3",
        "cordova-plugin-geolocation": "^2.4.3",
        "cordova-plugin-mauron85-background-geolocation": "^2.2.5",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "cordova-plugin-x-socialsharing": "^5.1.8",
        "cordova.plugins.diagnostic": "^3.6.5",
        "es6-promise-plugin": "^4.1.0",
        "ionic-angular": "3.3.0",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "3.0.0",
        "meteor-node-stubs": "^0.2.8",
        "meteor-rxjs": "^0.4.7",
        "phonegap-plugin-push": "^1.10.5",
        "rxjs": "5.1.1",
        "sw-toolbox": "3.6.0",
        "tmp": "0.0.31",
        "zone.js": "0.8.11"
    },
    "devDependencies": {
        "@ionic/app-scripts": "1.3.7",
        "@ionic/cli-plugin-cordova": "1.5.0",
        "@ionic/cli-plugin-ionic-angular": "1.4.0",
        "@types/meteor": "^1.4.1",
        "@types/meteor-collection-hooks": "^0.8.1",
        "@types/meteor-publish-composite": "0.0.32",
        "@types/underscore": "^1.8.0",
        "ionic": "3.6.0",
        "meteor-typings": "^1.3.1",
        "typescript": "2.3.3",
        "typescript-extends": "^1.0.1"
    },
    "description": "SimpleRide: en easy way to share rides",
    "config": {
        "ionic_webpack": "./webpack.config.js"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-console": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {},
            "ionic-plugin-keyboard": {},
            "cordova-plugin-facebook4": {
                "APP_ID": "210372952755567",
                "APP_NAME": "Simple Ride"
            },
            "cordova-plugin-geolocation": {
                "GEOLOCATION_USAGE_DESCRIPTION": " "
            },
            "cordova-plugin-crosswalk-webview": {
                "XWALK_VERSION": "23+",
                "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
                "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-camera": {
                "CAMERA_USAGE_DESCRIPTION": " ",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": " "
            },
            "cordova-plugin-file": {},
            "cordova-plugin-x-socialsharing": {},
            "cordova.plugins.diagnostic": {},
            "cordova-plugin-mauron85-background-geolocation": {
                "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
                "CONTENT_AUTHORITY": "$PACKAGE_NAME",
                "ALWAYS_USAGE_DESCRIPTION": "This app requires background location tracking"
            },
            "phonegap-plugin-push": {
                "SENDER_ID": "93847795927"
            },
            "cordova-plugin-app-version": {}
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}